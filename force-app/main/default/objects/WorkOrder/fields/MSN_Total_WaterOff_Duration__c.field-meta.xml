<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MSN_Total_WaterOff_Duration__c</fullName>
    <description>This calculates the total duration water has been switched off on a job. This is used for KPI reporting e.g. if water has been turned off longer than 5 or respectively 8 hours.</description>
    <externalId>false</externalId>
    <formula>( Actual_water_on_time__c - Actual_water_off_time__c )*24*60</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Total Water Off time (in minutes)</inlineHelpText>
    <label>Total Water Off Duration</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
